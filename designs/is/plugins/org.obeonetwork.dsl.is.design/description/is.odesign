<?xml version="1.0" encoding="ASCII"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.obeo.fr/dsl/viewpoint/description/1.1.0" xmlns:style="http://www.obeo.fr/dsl/viewpoint/description/style/1.1.0" xmlns:tool="http://www.obeo.fr/dsl/viewpoint/description/tool/1.1.0" name="Obeo Designer For IS" version="6.7.0.201302071200">
  <ownedViewpoints name="IS Views" modelFileExtension="ois">
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Overview Diagram" showOnStartup="true" domainClass="overview.Root" preconditionExpression="">
      <metamodel href="platform:/plugin/org.eclipse.emf.ecore/model/Ecore.ecore#/"/>
      <metamodel href="platform:/plugin/org.obeonetwork.dsl.environment/model/environment.ecore#/"/>
      <metamodel href="platform:/plugin/org.obeonetwork.dsl.entity/model/entity.ecore#/"/>
      <metamodel href="platform:/plugin/org.obeonetwork.dsl.soa/model/soa.ecore#/"/>
      <metamodel href="platform:/plugin/org.obeonetwork.dsl.cinematic/model/cinematic.ecore#/"/>
      <defaultLayer name="Overview">
        <nodeMappings name="RootEObject" semanticCandidatesExpression="&lt;%current.eContents()%>" doubleClickDescription="//@ownedViewpoints[name='IS%20Views']/@ownedRepresentations[name='Overview%20Diagram']/@defaultLayer/@toolSections.0/@subSections[name='Usability']/@ownedTools[name='Open%20main%20diagram']" domainClass="EObject">
          <style xsi:type="style:BundledImageDescription" labelExpression="null">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
          <conditionnalStyles predicateExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;soa.System&quot;%>">
            <style xsi:type="style:BundledImageDescription" labelExpression="SOA&lt;%if (name.length() > 0){%>::&lt;%}%>&lt;%name%>" sizeComputationExpression="5">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='blue']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_blue']"/>
            </style>
          </conditionnalStyles>
          <conditionnalStyles predicateExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;entity.Root&quot;%>">
            <style xsi:type="style:BundledImageDescription" labelExpression="Entity&lt;%if (name.length() > 0){%>::&lt;%}%>&lt;%name%>" sizeComputationExpression="5">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='orange']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_orange']"/>
            </style>
          </conditionnalStyles>
          <conditionnalStyles predicateExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;cinematic.CinematicRoot&quot;%>">
            <style xsi:type="style:BundledImageDescription" labelExpression="Cinematic&lt;%if (name.length() > 0){%>::&lt;%}%>&lt;%name%>" sizeComputationExpression="5">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_purple']"/>
            </style>
          </conditionnalStyles>
        </nodeMappings>
        <toolSections name="Overview">
          <subSections name="Create">
            <ownedTools xsi:type="tool:NodeCreationDescription" name="Entities Part" nodeMappings="//@ownedViewpoints[name='IS%20Views']/@ownedRepresentations[name='Overview%20Diagram']/@defaultLayer/@nodeMappings[name='RootEObject']" iconPath="/org.obeonetwork.dsl.entity.edit/icons/full/obj16/Root.gif">
              <variable name="container"/>
              <viewVariable name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:CreateInstance" typeName="entity.Root" referenceName="ownedElements">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="Root_&lt;%eContainer.ownedElements.filter(&quot;entity.Root&quot;).nSize%>"/>
                </firstModelOperations>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:NodeCreationDescription" name="Cinematic Part" nodeMappings="//@ownedViewpoints[name='IS%20Views']/@ownedRepresentations[name='Overview%20Diagram']/@defaultLayer/@nodeMappings[name='RootEObject']" iconPath="/org.obeonetwork.dsl.cinematic.edit/icons/full/obj16/CinematicRoot.gif">
              <variable name="container"/>
              <viewVariable name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:CreateInstance" typeName="cinematic.CinematicRoot" referenceName="ownedElements">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="Root_&lt;%eContainer.ownedElements.filter(&quot;cinematic.CinematicRoot&quot;).nSize%>"/>
                </firstModelOperations>
              </initialOperation>
            </ownedTools>
            <ownedTools xsi:type="tool:NodeCreationDescription" name="SOA Part" nodeMappings="//@ownedViewpoints[name='IS%20Views']/@ownedRepresentations[name='Overview%20Diagram']/@defaultLayer/@nodeMappings[name='RootEObject']" iconPath="/org.obeonetwork.dsl.soa.edit/icons/full/obj16/System.gif">
              <variable name="container"/>
              <viewVariable name="containerView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:CreateInstance" typeName="soa.System" referenceName="ownedElements">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="System_&lt;%eContainer.ownedElements.filter(&quot;soa.System&quot;).nSize%>"/>
                </firstModelOperations>
              </initialOperation>
            </ownedTools>
          </subSections>
          <subSections name="Usability">
            <ownedTools xsi:type="tool:DoubleClickDescription" name="Open main diagram" mappings="//@ownedViewpoints[name='IS%20Views']/@ownedRepresentations[name='Overview%20Diagram']/@defaultLayer/@nodeMappings[name='RootEObject']">
              <element name="element"/>
              <elementView name="elementView"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="&lt;%$element%>">
                  <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;soa.System&quot;%>">
                    <subModelOperations xsi:type="tool:Navigation" createIfNotExistent="true">
                      <diagramDescription href="../../org.obeonetwork.dsl.soa.design/description/soa.odesign#//@ownedViewpoints[name='SOA%20Views']/@ownedRepresentations[name='SOA%20Diagram']"/>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;entity.Root&quot;%>">
                    <subModelOperations xsi:type="tool:Navigation" createIfNotExistent="true">
                      <diagramDescription href="../../org.obeonetwork.dsl.entity.design/description/entity.odesign#//@ownedViewpoints[name='Entity%20Views']/@ownedRepresentations[name='Entities%20Namespaces%20Hierarchy']"/>
                    </subModelOperations>
                  </subModelOperations>
                  <subModelOperations xsi:type="tool:If" conditionExpression="&lt;%eClass.ePackage.name+&quot;.&quot;+eClass.name==&quot;cinematic.CinematicRoot&quot;%>">
                    <subModelOperations xsi:type="tool:Navigation" createIfNotExistent="true">
                      <diagramDescription href="platform:/plugin/org.obeonetwork.dsl.cinematic.design/description/cinematic.odesign#//@ownedViewpoints[name='Cinematic%20Views']/@ownedRepresentations[name='Package%20Diagram']"/>
                    </subModelOperations>
                  </subModelOperations>
                </firstModelOperations>
              </initialOperation>
            </ownedTools>
          </subSections>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Reused Tools" domainClass="soa.System" preconditionExpression="false">
      <defaultLayer name="Default">
        <toolSections name="Usability Tools">
          <ownedTools xsi:type="tool:DirectEditLabel" documentation="Edit the name feature of the element" name="Reused Name Direct Edit">
            <mask mask="{0}"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="&lt;%$0%>"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>

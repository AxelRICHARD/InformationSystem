<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.obeonetwork.org/dsl/cinematic/view/1.0.0"
            class="org.obeonetwork.dsl.cinematic.view.providers.ViewEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.obeonetwork.dsl.cinematic.flow.providers.FlowPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.obeonetwork.dsl.cinematic.view.providers.ViewPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="org.obeonetwork.dsl.cinematic.providers.CinematicPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="org.obeonetwork.dsl.cinematic.view.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="org.obeonetwork.dsl.cinematic.view.properties">
         <propertyTab
               id="ViewContainer"
               label="ViewContainer"
               category="default">
         </propertyTab>

         <propertyTab
               id="Metadatas"
               label="Metadatas"
               category="extended"
               afterTab="ViewContainer">
         </propertyTab>

         <propertyTab
               id="ViewElement"
               label="ViewElement"
               category="default">
         </propertyTab>

         <propertyTab
               id="ViewAction"
               label="ViewAction"
               category="default">
         </propertyTab>

         <propertyTab
               id="ViewEvent"
               label="ViewEvent"
               category="default">
         </propertyTab>

         <propertyTab
               id="EntityBinding"
               label="EntityBinding"
               category="default">
         </propertyTab>

         <propertyTab
               id="EntityPropertyBinding"
               label="EntityPropertyBinding"
               category="default">
         </propertyTab>

         <propertyTab
               id="DTOPropertyBinding"
               label="DTOPropertyBinding"
               category="default">
         </propertyTab>

         <propertyTab
               id="DTOBinding"
               label="DTOBinding"
               category="default">
         </propertyTab>

         <propertyTab
               id="ViewContainerReference"
               label="ViewContainerReference"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
